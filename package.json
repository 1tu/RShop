{
  "name": "rshop",
  "version": "0.0.1",
  "description": "Replicative shop with CRM",
  "main": "server.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "start:be": "node index.js",
    "start:be:watch": "nodemon",
    "start:be:prod": "node dist/server.js",
    "prestart:be:prod": "tsc -p tsconfig.server.json",
    "build:fe": "cross-env NODE_ENV=production npm run clean && npm run compile:fe",
    "build:fe:parts": "npm run sass && npm run compile",
    "clean": "rimraf public && rimraf coverage",
    "compile:fe": "webpack --config config/webpack/webpack.config.prod.js",
    "lint": "tslint src/**/*.ts",
    "db:drop": "typeorm schema:drop",
    "db:seed": "node seed.js"
  },
  "keywords": ["vuejs"],
  "author": "itu",
  "license": "UNLICENSED",
  "private": true,
